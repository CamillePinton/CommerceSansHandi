  },

  "tags": [
    {
      "name": "Commerce Sans Handi",
      "description": "Trouver des commerces sans PMR",
      "externalDocs": {
        "description": "Commerces Pays de la Loire",
        "url": "https://data.paysdelaloire.fr/explore/dataset/commerces-export-openstreetmap-france/table/?disjunctive.type&disjunctive.region&disjunctive.departement&disjunctive.commune"
      }
    },
    {
      "name": "Autres",
      "description": ""
    }
  ],
  
  "paths": {
    "/": {
      "get": {
        "tags": ["Application"],
        "summary": "Page d'accueil",
        "description": "Page d'accueil de Commerce Sans Handi",
        "responses": {
          "200": {
            "description": "Successful response"
          }
        }
      }
    },
    "/commerces": {
      "get": {
        "tags": ["Application"],
        "summary": "Retourne tous les commerces du Pays de Loire",
        "description": "Liste tous les commerces",
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": []
              }
            }
          }
        }
      }
    },
    "/horaires/<codeArret>/<numLigne>/<sens>": {
      "get": {
        "tags": ["Application"],
        "summary": "Get Horaires",
        "description": "Get theoretical schedules for a specific bus stop, line, and direction.",
        "parameters": [
          {
            "name": "codeArret",
            "in": "path",
            "required": true,
            "description": "Code of the bus stop",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "numLigne",
            "in": "path",
            "required": true,
            "description": "Number of the bus line",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sens",
            "in": "path",
            "required": true,
            "description": "Direction of the bus",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": "Erreur serveur"
              }
            }
          }
        }
      }
    },
    "/tempsattente/<codeArret>": {
      "get": {
        "tags": ["Application"],
        "summary": "Get Temps Attente",
        "description": "Get waiting times for a specific bus stop.",
        "parameters": [
          {
            "name": "codeArret",
            "in": "path",
            "required": true,
            "description": "Code of the bus stop",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": [{"arret":{"codeArret":"BATI1"},"dernierDepart":"false","infotrafic":false,"ligne":{"numLigne":"C6","typeLigne":3},"sens":1,"temps":"4mn","tempsReel":"true","terminus":"Chantrerie - Grandes Ecoles"}]}
            }
          }
        }
      }
    },     
    "/tempsattente/<codeAr>/<nbPassages>": {
      "get": {
        "tags": [
          "Application"
        ],
        "summary": "Get waiting time for a stop and number of passages",
        "description": "Retrieve the waiting time for a specific stop and number of passages",
        "parameters": [
          {
            "name": "codeArret",
            "in": "path",
            "required": true,
            "description": "Stop code",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "nbPassages",
            "in": "path",
            "required": true,
            "description": "Number of passages",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example":
                  [{"arret":{"codeArret":"BATI2"},"dernierDepart":"false","infotrafic":false,"ligne":{"numLigne":"C6","typeLigne":3},"sens":2,"temps":"4mn","tempsReel":"true","terminus":"Hermeland"}]
              }
            }
          }
        }
      }
    },
    "/tempsattente/<codeArret>/<nbPassages>/<numLigne>": {
      "get": {
        "tags": [
          "Application"
        ],
        "summary": "Get waiting time for a stop, number of passages, and line",
        "description": "Retrieve the waiting time for a specific stop, number of passages, and line",
        "parameters": [
          {
            "name": "codeArret",
            "in": "path",
            "required": true,
            "description": "Stop code",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "nbPassages",
            "in": "path",
            "required": true,
            "description": "Number of passages",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "numLigne",
            "in": "path",
            "required": true,
            "description": "Line number",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": [{"arret":{"codeArret":"BATI2"},"dernierDepart":"false","infotrafic":false,"ligne":{"numLigne":"C6","typeLigne":3},"sens":2,"temps":"4mn","tempsReel":"true","terminus":"Hermeland"}]
              }
            }
          }
        }
      }
    }
  },
  "securitySchemes": {
      "api_key": {
          "type": "apiKey",
          "name": "api_key",
          "in": "header"
      }
  }
}